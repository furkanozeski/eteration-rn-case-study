{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@CompositeComponents/*":[
        "src/Components/CompositeComponents/*"
      ],
      "@UIComponents/*":[
        "src/Components/UIComponents/*"
      ],
      "@Asset/*":[
        "src/Asset/*"
      ],
      "@Context/*":[
        "src/Context/*"
      ],
      "@Navigation/*":[
        "src/Navigation/*"
      ],
      "@Screens/*":[
        "src/Screens/*"
      ],
      "@root/*":[
        "./*"
      ],
      "@Routes/*":[
        "src/Routes/*"
      ]
    }
  }
}
